[submodule ".registry"]
	path = .registry
	url = https://github.com/r-universe-org/demo-registry
	shallow = true
	branch = HEAD
[submodule "jose"]
	path = jose
	url = https://github.com/jeroen/jose
	shallow = true
	branch = *release
	registered = true
[submodule "curl"]
	path = curl
	url = https://github.com/jeroen/curl
	shallow = true
	branch = v4.2
	registered = true
[submodule "stringdist"]
	path = stringdist
	url = https://github.com/markvanderloo/stringdist
	shallow = true
	subdir = pkg
	registered = true
[submodule "ggsegHO"]
	path = ggsegHO
	url = https://github.com/ggseg/ggsegHO
	shallow = true
	registered = true
