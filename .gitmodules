[submodule ".registry"]
	path = .registry
	url = https://github.com/r-universe-org/demo-registry
	shallow = true
	branch = HEAD
[submodule "jose"]
	path = jose
	url = https://github.com/r-lib/jose
	shallow = true
	branch = v1.1.0
	registered = true
[submodule "curl"]
	path = curl
	url = https://github.com/jeroen/curl
	shallow = true
	branch = v4.2
	registered = true
[submodule "stringdist"]
	path = stringdist
	url = https://github.com/markvanderloo/stringdist
	shallow = true
	subdir = pkg
	registered = true
[submodule "ggsegHO"]
	path = ggsegHO
	url = https://github.com/ggseg/ggsegHO
	shallow = true
	registered = true
[submodule "hdf5r"]
	path = hdf5r
	url = https://github.com/jeroen/hdf5r
	shallow = true
	branch = cranfix
	registered = true
[submodule "ggseg"]
	path = ggseg
	url = https://github.com/LCBC-UiO/ggseg
	shallow = true
	registered = false
[submodule "ggseg3d"]
	path = ggseg3d
	url = https://github.com/LCBC-UiO/ggseg3d
	shallow = true
	registered = false
